{
  "manifest_version": 3,
  "name": "Drupal Template DevTools",
  "version": "1.0",
  "description": "A Chrome DevTools extension for exploring Drupal templates",
  "permissions": [
    "declarativeContent"
  ],
  "devtools_page": "devtools.html",
